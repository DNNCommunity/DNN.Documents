      <releaseNotes>
        <![CDATA[ 
          <h3>Version 06.00.01</h3>
          <p>This release fixed a packaging issue with the SQL Scripts for users upgrading from 05.x</p>
        
          <h3>Version 06.00.00</h3>
          <p><strong>NOTE:</strong> If Upgrading from versions PRIOR to 05.00.00 upgrade to 05.00.00 FIRST!</p>
          
          <ul>
            <li>21437/22277 - Resolved issues with DNN 6.x and later Form Patterns</li>
            <li>24341 - Support For DotNetNuke 7.1 on Clean Installation</li>
            <li>21461 - Improved DotNetNuke Core Integration by allowing document description to be searchable</li>
            <li>21186 - Default Folder selection</li>
            <li>20471 - Display issues when using Category List ***</li>
            <li>23018 - Added option to allow "Force Download" to be set</li>
            <li>23016 - Support for Azure</li>
            <li>21588 - Resolved issues with non-english languages and custom CSS Classes</li>
          </ul>
          
          <p>*** Please see release notes on CodePlex for needed steps for legacy resolution of this issue.</p>
          
          <h3>Version 05.00.00</h3>
          
          <ul>
              <li>Updated to DNN 5.x manifest format</li>
              <li>Added clustered primary key on Documents tabl</li>
              <li>Resolved issues created by previous releases</li>
              <li>Resolved outstanding bugs</li>
          </ul>
        
        ]]>
      </releaseNotes>
      <dependencies>
        <dependency type="CoreVersion">06.00.02</dependency>
      </dependencies>
      <components>
        <component type="Script">
          <scripts>
            <basePath>DesktopModules\Documents</basePath>
            <script type="Install">
              <path>Providers\DataProviders\SQLDataProvider</path>
              <name>install.06.00.00.SqlDataProvider</name>
              <version>06.00.00</version>
            </script>
            <script type="Install">
              <path>Providers\DataProviders\SQLDataProvider</path>
              <name>06.00.00.SqlDataProvider</name>
              <version>06.00.00</version>
            </script>
            <script type="UnInstall">
              <path>Providers\DataProviders\SQLDataProvider</path>
              <name>Uninstall.SqlDataProvider</name>
              <version>06.00.00</version>
            </script>
          </scripts>
        </component>

        <component type="ResourceFile">
          <resourceFiles>
            <basePath>DesktopModules/Documents</basePath>
            <resourceFile>
              <name>Resources.zip</name>
            </resourceFile>
          </resourceFiles>
        </component>

        <component type="Module">
          <desktopModule>
            <moduleName>DNN_Documents</moduleName>
            <foldername>Documents</foldername>
            <businessControllerClass>DotNetNuke.Modules.Documents.DocumentController, DotNetNuke.Modules.Documents</businessControllerClass>
            <supportedFeatures>
              <supportedFeature type="Portable" />
              <supportedFeature type="Searchable" />
            </supportedFeatures>
            <moduleDefinitions>
              <moduleDefinition>
                <friendlyName>Documents</friendlyName>
                <defaultCacheTime>0</defaultCacheTime>
                <moduleControls>
                  <moduleControl>
                    <controlKey />
                    <controlSrc>DesktopModules/Documents/Document.ascx</controlSrc>
                    <supportsPartialRendering>False</supportsPartialRendering>
                    <controlTitle />
                    <controlType>View</controlType>
                    <iconFile />
                    <helpUrl></helpUrl>
                    <viewOrder>0</viewOrder>
                  </moduleControl>
                  <moduleControl>
                    <controlKey>Edit</controlKey>
                    <controlSrc>DesktopModules/Documents/EditDocs.ascx</controlSrc>
                    <supportsPartialRendering>False</supportsPartialRendering>
                    <controlTitle>Edit Documents</controlTitle>
                    <controlType>Edit</controlType>
                    <iconFile />
                    <helpUrl></helpUrl>
                    <viewOrder>0</viewOrder>
                  </moduleControl>
                  <moduleControl>
                    <controlKey>Settings</controlKey>
                    <controlSrc>DesktopModules/Documents/EditDocumentsSettings.ascx</controlSrc>
                    <supportsPartialRendering>False</supportsPartialRendering>
                    <controlTitle>Documents Settings</controlTitle>
                    <controlType>Edit</controlType>
                    <iconFile />
                    <helpUrl></helpUrl>
                    <viewOrder>0</viewOrder>
                  </moduleControl>
                </moduleControls>
              </moduleDefinition>
            </moduleDefinitions>
          </desktopModule>
        </component>
        
        <component type="Assembly">
          <assemblies>
            <basePath>bin</basePath>
            <assembly>
              <path>bin</path>
              <name>DotNetNuke.Modules.Documents.dll</name>
            </assembly>
          </assemblies>
        </component>

      </components>
    </package>
  </packages>
</dotnetnuke>
